{
  "name": "postcss-npm",
  "version": "1.1.0",
  "description": "Import CSS from npm modules using rework",
  "main": "./lib/index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "ava",
    "coverage": "nyc npm test",
    "codeclimate": "npm run coverage && nyc report --reporter=text-lcov | codeclimate-test-reporter",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GarthDB/postcss-npm.git"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "import",
    "npm",
    "modules"
  ],
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "author": "Garth Braithwaite <garthdb@gmail.com> (http://garthdb.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GarthDB/postcss-npm/issues"
  },
  "homepage": "https://github.com/GarthDB/postcss-npm#readme",
  "dependencies": {
    "debug": "^2.4.1",
    "postcss": "^5.1.2",
    "resolve": "^1.1.7"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "codeclimate-test-reporter": "^0.3.3",
    "convert-source-map": "^1.3.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "node-sass": "^3.8.0",
    "nyc": "^8.1.0",
    "perfectionist": "^2.2.0",
    "postcss-inherit-parser": "^0.1.4",
    "source-map": "^0.5.6",
    "topcoat-utils": "^0.4.1"
  }
}
